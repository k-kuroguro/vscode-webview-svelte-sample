{
   "name": "vscode-webview-svelte-sample",
   "displayName": "",
   "description": "VSCode webview sample created by Svelte",
   "version": "0.0.1",
   "engines": {
      "vscode": "^1.57.0"
   },
   "categories": [
      "Other"
   ],
   "activationEvents": [
      "onCommand:vscode-webview-svelte-sample.showCounter"
   ],
   "main": "./dist/extension.js",
   "contributes": {
      "commands": [
         {
            "command": "vscode-webview-svelte-sample.showCounter",
            "title": "Webview Svelte Sample: Show Counter"
         }
      ]
   },
   "scripts": {
      "vscode:prepublish": "npm run package",
      "compile": "webpack",
      "watch": "webpack --watch",
      "package": "webpack --mode production --devtool hidden-source-map",
      "test-compile": "tsc -p ./",
      "test-watch": "tsc -watch -p ./",
      "pretest": "npm run test-compile && npm run lint",
      "lint": "eslint src --ext ts,svelte",
      "test": "node ./out/test/runTest.js"
   },
   "devDependencies": {
      "@types/glob": "^7.1.3",
      "@types/mocha": "^8.0.4",
      "@types/node": "^12.11.7",
      "@types/vscode": "^1.57.0",
      "@types/vscode-webview": "^1.57.0",
      "@typescript-eslint/eslint-plugin": "^4.14.1",
      "@typescript-eslint/parser": "^4.14.1",
      "csp-html-webpack-plugin": "^5.1.0",
      "css-loader": "^5.2.6",
      "ejs-compiled-loader": "^3.1.0",
      "eslint": "^7.19.0",
      "eslint-plugin-svelte3": "^3.2.0",
      "glob": "^7.1.6",
      "html-loader": "^2.1.2",
      "html-webpack-plugin": "^5.3.1",
      "mini-css-extract-plugin": "^1.6.0",
      "mocha": "^8.2.1",
      "sass": "^1.34.1",
      "sass-loader": "^12.1.0",
      "svelte": "^3.38.2",
      "svelte-loader": "^3.1.1",
      "svelte-preprocess": "^4.7.3",
      "ts-loader": "^8.0.14",
      "typescript": "^4.1.3",
      "vscode-test": "^1.5.0",
      "webpack": "^5.19.0",
      "webpack-cli": "^4.4.0"
   }
}
